{
  "name": "foundry-scripts",
  "version": "0.3.1",
  "description": "deploymentr, ds-test, cheat codes, and scripts for foundry",
  "main": "index.js",
  "directories": {
    "scripts": "scripts",
    "src": "src"
  },
  "files": [
    "justfile",
    "/scripts/**/*.sh",
    "/src/**/*.sol",
    "/out/src/*.json",
    "!/src/__test__/**/*"
  ],
  "scripts": {
    "solc": "solc --bin --abi --optimize --overwrite --bin-runtime --allow-paths src/mocks/ -o out/src/",
    "mk": "just build",
    "gen:salt": "node gen-salt.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sambacha/foundry-scripts.git"
  },
  "keywords": [
    "foundry",
    "scripts",
    "ethereum",
    "solidity"
  ],
  "author": "SEE CONTRIBUTORS",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/sambacha/foundry-scripts/issues"
  },
  "homepage": "https://github.com/sambacha/foundry-scripts#readme",
  "devDependencies": {
    "dotenv": "^16.0.0",
    "ethers": "^5.6.2"
  },
}
